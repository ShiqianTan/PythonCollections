    "def isPalindrome(s):\n",
    "\tif len(s) <= 2:\n",
    "\t\treturn s[0] == s[-1]\n",
    "\tif s[0] == s[-1]:\n",
    "\t\treturn isPalindrome(s[1:-1])\n",
    "\telse:\n",
    "\t\treturn False"
   ]
